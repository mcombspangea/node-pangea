{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["prettier", "@gitlab"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "requireConfigFile": false
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"]
  }
}
